{
  "name": "ng-flex-layout",
  "description": "Angular Flex Layout (DOIT fork)",
  "version": "20.0.0",
  "homepage": "https://github.com/alessiobianchini/flex-layout",
  "bugs": "https://github.com/alessiobianchini/flex-layout/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alessiobianchini/flex-layout"
  },
  "license": "MIT",
  "engines": {
    "node": "^20.9.0 || ^22.16.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-flex-layout",
    "build-demo-app": "ng build demo-app",
    "publish": "cd dist/releases/flex-layout && npm publish --access public --no-git-checks",
    "demo:serve": "ng serve demo-app",
    "stamp": "ts-node tools/package-tools/version-placeholders.ts --module=commonjs",
    "build:universal-demo-app": "ng run universal-demo-app:build:production && ng run universal-demo-app:server",
    "serve:universal-demo-app": "ng run universal-demo-app:serve-ssr",
    "test": "ng test ng-flex-layout",
    "test:ssr": "webpack --config test/webpack-spec-ssr-bundle.js && jasmine --config=test/jasmine-ssr.json",
    "lint": "eslint projects/libs/**/*.{js,jsx,ts,tsx,html} --fix",
    "release": "yarn build && yarn stamp",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "serve:ssr:universal-demo-app": "node dist/universal-demo-app/server/server.mjs"
  },
  "private": true,
  "requiredAngularVersion": ">=20.0.0",
  "dependencies": {
    "@angular/animations": "~20.0.3",
    "@angular/common": "~20.0.3",
    "@angular/compiler": "~20.0.3",
    "@angular/core": "~20.0.3",
    "@angular/forms": "~20.0.3",
    "@angular/material": "~20.0.3",
    "@angular/platform-browser": "~20.0.3",
    "@angular/platform-browser-dynamic": "~20.0.3",
    "@angular/platform-server": "~20.0.3",
    "@angular/router": "~20.0.3",
    "@angular/ssr": "^20.0.2",
    "express": "^4.21.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.2",
    "@angular-eslint/builder": "~20.1.0",
    "@angular-eslint/eslint-plugin": "~20.1.0",
    "@angular-eslint/eslint-plugin-template": "~20.1.0",
    "@angular-eslint/schematics": "~20.1.0",
    "@angular-eslint/template-parser": "~20.1.0",
    "@angular/build": "^20.0.2",
    "@angular/cdk": "~20.0.3",
    "@angular/cli": "~20.0.2",
    "@angular/compiler-cli": "~20.0.3",
    "@ngtools/webpack": "~20.0.2",
    "@types/express": "^4.17.23",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^20.19.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "browser-sync": "^3.0.4",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.1.0",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.4",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-parallel": "^0.3.1",
    "ng-packagr": "~20.0.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "stylelint": "^15.11.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "webpack": "^5.99.9"
  },
